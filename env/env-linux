# Misc Paths
export DOTFILES="$HOME/workspace/personal/repos/.dotfiles"
export DOTFILES_WORK="$HOME/cloud/work/setup/dotfiles"
export DOTFILES_CLOUD="$HOME/cloud/personal/setup/dotfiles"
export WORKSPACE="$HOME/workspace"
export REPOS_PERSONAL="$HOME/workspace/personal/repos"
export REPOS_WORK="$HOME/workspace/work/repos"
export LOG_PATH="$HOME/logs"

# XDG
export XDG_CONFIG_HOME="$HOME/.config"
export XDG_DATA_HOME="$XDG_CONFIG_HOME/local/share"
export XDG_CACHE_HOME="$XDG_CONFIG_HOME/cache"
