{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palettes": {
    "list": {
      "dark": {
        "background": "#FDF6E3",
        "black": "#56595C",
        "blackAlt": "#6B6F73",
        "blue": "#2E8BCE",
        "brightBlack": "#45484B",
        "brightBlue": "#2176C7",
        "brightCyan": "#259286",
        "brightGreen": "#738A04",
        "brightPurple": "#C61C6F",
        "brightRed": "#BD3613",
        "brightWhite": "#999690",
        "brightYellow": "#A57705",
        "cursorColor": "#536870",
        "cyan": "#32A198",
        "foreground": "#536870",
        "foregroundDark": "p:whiteAlt",
        "foregroundLight": "p:whiteAlt",
        "green": "#85981C",
        "purple": "#D13A82",
        "purpleAlt": "#6C71C4",
        "red": "#C94C22",
        "selectionBackground": "#EEE8D5",
        "white": "#A6A49D",
        "whiteAlt": "#BFBDB5",
        "yellow": "#B4881D"
      },
      "light": {
        "background": "#FDF6E3",
        "black": "#56595C",
        "blackAlt": "#6B6F73",
        "blue": "#2E8BCE",
        "brightBlack": "#45484B",
        "brightBlue": "#2176C7",
        "brightCyan": "#259286",
        "brightGreen": "#738A04",
        "brightPurple": "#C61C6F",
        "brightRed": "#BD3613",
        "brightWhite": "#999690",
        "brightYellow": "#A57705",
        "cursorColor": "#536870",
        "cyan": "#32A198",
        "foreground": "#536870",
        "foregroundDark": "p:whiteAlt",
        "foregroundLight": "p:whiteAlt",
        "green": "#85981C",
        "purple": "#D13A82",
        "purpleAlt": "#6C71C4",
        "red": "#C94C22",
        "selectionBackground": "#EEE8D5",
        "white": "#A6A49D",
        "whiteAlt": "#BFBDB5",
        "yellow": "#B4881D"
      }
    },
    "template": "{{ if eq .Env.POSH_LIGHT_THEME \"1\" }}light{{ else }}dark{{ end }}"
  },
  "secondary_prompt": {
    "template": "\u2570\u2500❯ ",
    "foreground": "p:brightBlack",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "❯ ",
    "foreground": "p:black",
    "background": "transparent"
  },
  "console_title_template": "PowerShell",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "\uf300 ",
            "arch": "\uf303 ",
            "cache_duration": "none",
            "centos": "\uf304 ",
            "debian": "\uf306 ",
            "linux": "\ue712 ",
            "ubuntu": "\uf31c ",
            "windows": "\ue70f "
          },
          "leading_diamond": "\ue0b6",
          "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }} ",
          "foreground": "p:foregroundLight",
          "background": "p:black",
          "type": "os",
          "style": "diamond"
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "p:black",
          "background": "p:whiteAlt",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "{{ if .SSHSession }} {{ end }}{{ .HostName }} "
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf09c ",
          "foreground": "p:foregroundDark",
          "background": "p:red",
          "type": "root",
          "style": "diamond",
          "trailing_diamond": ""
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "pwsh": "PS",
              "zsh": "ZSH"
            }
          },
          "template": " \uf489  {{ .Name }} {{ .Version }} ",
          "foreground": "p:foregroundDark",
          "powerline_symbol": "\ue0b0",
          "background": "p:brightBlue",
          "type": "shell",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": " \ue73c {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Major}}.{{.Minor }} ",
          "foreground": "p:foregroundDark",
          "background": "p:blue",
          "type": "python",
          "style": "diamond",
          "trailing_diamond": "\ue0b0"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue753 {{ if .Error }}???{{ else }}{{ .Major}}.{{.Minor }}{{ end }} ",
          "foreground": "p:foregroundDark",
          "background": "p:blue",
          "type": "angular",
          "fetch_version": true,
          "style": "diamond",
          "trailing_diamond": "<p:foregroundDark,p:blue></>"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue718 {{ if .Error }}???{{ else }}{{ .Major}}.{{.Minor }}{{ end }} ",
          "foreground": "p:foregroundDark",
          "background": "p:blue",
          "type": "node",
          "style": "diamond",
          "trailing_diamond": "\ue0b0"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue77f {{ if .Error }}???{{ else }}{{ .Major}}.{{.Minor }}{{ end }} ",
          "foreground": "p:foregroundDark",
          "background": "p:blue",
          "type": "dotnet",
          "style": "diamond",
          "trailing_diamond": "\ue0b0"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " \uf07c  {{ .Path }} ",
          "foreground": "p:foregroundDark",
          "background": "p:brightGreen",
          "type": "path",
          "style": "diamond",
          "leading_diamond": "<transparent,p:brightGreen>\ue0b0</>",
          "trailing_diamond": "\ue0b0"
        },
        {
          "properties": {
            "branch_icon": "\ue0a0 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<p:foregroundLight> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "p:foregroundDark",
          "trailing_diamond": "\ue0b0",
          "background": "p:green",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:green{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:green{{ end }}",
            "{{ if gt .Ahead 0 }}p:purple{{ end }}",
            "{{ if gt .Behind 0 }}p:purpleAlt{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if eq .Env.POSH_FULL_PROMPT \"1\" }}\u256d\u2500 {{ end }}",
          "foreground": "p:brightBlack",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "\ue641 15:04:05</> |</> \uf073 2 Jan, Monday"
          },
          "template": "{{ if eq .Env.POSH_FULL_PROMPT \"1\" }}{{ .CurrentDate | date .Format }} <p:whiteAlt>|</>{{ end }}",
          "foreground": "p:brightYellow",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "cycle": [
              "p:white",
              "p:brightWhite",
              "p:blackAlt",
              "p:black",
              "p:brightBlack"
            ],
            "cycle_folder_separator": true,
            "folder_icon": " \uf07c ",
            "folder_separator_icon": " \udb80\udc54 ",
            "home_icon": "\ue617 ",
            "mapped_locations": {
              "C:\\temp": "\ue799"
            },
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "leading_diamond": "<p:whiteAlt> \uf07b </><p:whiteAlt> in </>",
          "template": "{{ if eq .Env.POSH_FULL_PROMPT \"1\" }} {{ .Path }} {{ end }}",
          "foreground": "p:whiteAlt",
          "type": "path",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if eq .Env.POSH_FULL_PROMPT \"1\" }}\u2570\u2500{{ end }}",
          "foreground": "p:brightBlack",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "❯ ",
          "foreground": "p:brightBlack",
          "type": "status",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
